<script>
function registerNow(){
  const name = document.getElementById('name').value || 'Атын көрсетпеді';
  const people = document.getElementById('people').value || '1';
  const note = document.getElementById('note').value || '';
 const raw = '87075808507';
  const intl = raw.replace(/^8/,'7');

  const msg = encodeURIComponent(`Ernaz Tur – Тіркелемін. Аты: ${name}. Қатысушылар: ${people}. ${note}`);
  window.open(`https://wa.me/${intl}?text=${msg}`,'_blank');
}

function selectTag(tag){
  const note = document.getElementById('note');
  if(!note.value.includes(tag)){
    note.value += (note.value ? ', ' : '') + tag;
  }
}

function openLightbox(src){
  const lb = document.getElementById('lightbox');
  const img = document.getElementById('lightbox-img');
  img.src = src;
  lb.style.display = 'flex';
  lb.onclick = () => { lb.style.display='none'; img.src=''; }
}

function toggleFaq(el){
  const answer = el.parentNode.querySelector('.faq-answer');
  if(answer.style.display === 'block'){
    answer.style.display='none';
  } else {
    answer.style.display='block';
  }
}

function subscribe(){
  const email = document.getElementById('newsletter').value;
  if(!email) return alert('Email енгізіңіз');
  alert('Рахмет! Сіз жазылдыңыз: ' + email);
}

// MAP
(function initMap(){
  const map = L.map('map').setView([43.2389, 76.8897], 12);

  L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
    maxZoom: 19,
  }).addTo(map);

  const places = [
    {name:'Шымбұлақ',coords:[43.1667,77.1000]},
    {name:'Медеу',coords:[43.1356,77.1111]},
    {name:'Көлсай',coords:[43.1999,78.3211]},
  ];

  places.forEach(p=>{
    const marker = L.marker(p.coords).addTo(map);
    marker.bindPopup(`<strong>${p.name}</strong>`);
    marker.on('click',()=>selectTag(p.name));
  });
})();
</script>
